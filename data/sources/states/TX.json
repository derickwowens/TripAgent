{
  "stateCode": "TX",
  "stateName": "Texas",
  "parkAuthority": {
    "name": "Texas Parks and Wildlife Department",
    "abbreviation": "TPWD",
    "website": "https://tpwd.texas.gov/state-parks/"
  },
  "dataSources": {
    "boundaries": {
      "type": "arcgis",
      "url": "https://tpwd.texas.gov/server/rest/services/Parks",
      "description": "Texas Parks and Wildlife GIS services"
    },
    "parkList": {
      "type": "pad_us",
      "filters": {
        "State_Nm": "TX",
        "Mang_Type": "STAT",
        "Des_Tp": ["SP", "SRA", "SHCA", "SNA", "SHS"]
      }
    },
    "trails": {
      "type": "arcgis_rest",
      "url": "https://tpwd.texas.gov/server/rest/services/Parks/TexasStateParksTrails/MapServer/0",
      "queryParams": {
        "where": "1=1",
        "outFields": "*",
        "returnGeometry": true,
        "outSR": 4326,
        "f": "json"
      },
      "description": "Official Texas State Parks trails with use types (hiking, biking, equestrian, etc.)"
    },
    "campgrounds": {
      "type": "reserve_america",
      "stateFilter": "TX"
    }
  },
  "reservationSystem": {
    "provider": "Texas State Parks Reservations",
    "providerType": "reserve_america",
    "baseUrl": "https://texas.reserveworld.com/",
    "deepLinkPattern": "https://texas.reserveworld.com/GeneralAvailabilityCalendar.aspx?PlaceId={parkId}",
    "searchUrl": "https://texas.reserveworld.com/"
  },
  "urlPatterns": {
    "officialParkPage": "https://tpwd.texas.gov/state-parks/{slug}",
    "parkMaps": "https://tpwd.texas.gov/state-parks/park-information/maps"
  },
  "designationTypes": {
    "SP": {
      "code": "SP",
      "name": "State Park",
      "pluralName": "State Parks"
    },
    "SRA": {
