  "fees": [
    {
      "title": "Entrance - Private Vehicle",
      "description": "Admits a private non-commercial vehicle for 1-7 days.",
      "cost": "25.00",
      "type": "entrance"
    },
    {
      "title": "Entrance - Motorcycle",
      "description": "Valid for 7 days. Admits up to 2 private, non-commercial motorcycles with up to 4 total passengers.",
      "cost": "20.00",
      "type": "entrance"
    },
    {
      "title": "Entrance - Per Person",
      "description": "Admits a single individual walking/bicycling for 1-7 days.",
      "cost": "15.00",
      "type": "entrance"
    },
    {
      "title": "Entrance - Non-commercial Groups",
      "description": "Admits vehicles at the per person rate for groups using vehicles for 12 or more people. The $15 cost is per person ages 16+, not to exceed the flat rate paid commercial tours for same type of vehicle.",
      "cost": "15.00",
      "type": "entrance"
    },
    {
      "title": "Entrance - Education/Academic Groups",
      "description": "Admits education/academic groups as long as a fee waiver form is filled out and approved.",
      "cost": "0.00",
      "type": "entrance"
    },
    {
      "title": "Commercial Entrance - Sedan",
      "description": "Admits a sedan of 1-6 people.",
      "cost": "25.00",
      "type": "entrance"
    },
    {
      "title": "Commercial Entrance - Van",
      "description": "Admits a van of 7-16 persons.",
      "cost": "50.00",
      "type": "entrance"
    },
    {
      "title": "Commercial Entrance - Mini-bus",
      "description": "Admits a mini-bus of 16-25 persons.",
      "cost": "60.00",
      "type": "entrance"
    },
    {
      "title": "Commercial Entrance - Motor Coach",
      "description": "Admits a motor coach of 26+ persons.",
      "cost": "150.00",
      "type": "entrance"
    }
  ],
  "climate": {
    "weatherDescription": "Spring (April, May and June): Daytime highs 80 °F to 98 °F. Nighttime lows 50 °F to 66 °F Summer* (July, August and September): Daytime highs 89 °F to 103 °F (It can get up to 117 °F.) Fall (October, November and December): Daytime highs 59 °F to 83 °F. Nighttime lows 32 °F to 50 °F. Winter (January, February and March) Daytime highs 57 °F to 69 °F. Nighttime lows 30 °F to 40 °F. *Summer can be a dangerous time due to excessive heat. Some trails are closed for public safety."
  },
